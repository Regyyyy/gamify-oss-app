{
    "version": 2,
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "public/build",
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/build/(.*)",
        "dest": "/build/$1" 
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "/app.jsx"
      }
    ],
    "devCommand": "npm run dev"
  }